function find_name(_array){
	switch _array
		{
		case 0:
			return "Brandon";
			break;
		case 1:
			return "Shaun";
			break;
		case 2:
			return "Randy";
			break;
		case 3:
			return "Miranda";
			break;
		case 4:
			return "Violet";
			break;
		case 5:
			return "Clayton";
			break;
		case 6:
			return "Jonathan"
			break;
		case 7:
			return "Sam"
			break;
		}
}